#!/bin/bash

source common

DESC="scan with verbose --minsize"

$DUPD_CMD scan --path `pwd`/files --minsize 10000 -v -v -v -v > /dev/null
checkrv $?

$DUPD_CMD report --cut `pwd`/files/ | grep -v "Duplicate report from database" > nreport
checkrv $?

sort nreport > report
diff report output.41
checkrv $?

tdone
