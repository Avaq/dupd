#!/usr/bin/env bash

source common

DESC="scan(files)"
$DUPD_CMD scan --path `pwd`/files -q $DUPD_CACHEOPT
checkrv $?

DESC="json report"
$DUPD_CMD report --format json --cut `pwd`/files/ $DUPD_CACHEOPT | tr ',' '\n' | sed -e s/\ ]$//g > nreport
checkrv $?

#
# The JSON output was split into lines above, not entirely realistic,
# but needed to have a consistent ordering to compare.

check_nreport output.78

tdone
