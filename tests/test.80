#!/usr/bin/env bash

source common

rm -f stx

DESC="scan(files) testing count stats (HDD)"
$DUPD_CMD scan --path `pwd`/files -q --hdd --stats-file stx
checkrv $?

rm -f nreport report
grep stats_duplicate_files stx > nreport
grep 'stats_duplicate_groups ' stx >> nreport

check_nreport output.80

tdone
