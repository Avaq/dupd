#!/bin/bash

source common

DESC="ls ignoring uniques with --no-unique"

$DUPD_CMD scan --path `pwd`/files -q --uniques --x-small-buffers
checkrv $?

$DUPD_CMD ls --x-small-buffers -v --no-unique --cut `pwd`/files/ --path `pwd`/files/ > nreport
checkrv $?

sort nreport > report
diff report output.29
checkrv $?

tdone
