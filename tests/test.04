#!/usr/bin/env bash

source common

DESC="scan -v -v -v"
$DUPD_CMD scan --path `pwd`/files -v -v -v $DUPD_CACHEOPT > /dev/null
checkrv $?

DESC="generate report"
$DUPD_CMD report --cut `pwd`/files/ $DUPD_CACHEOPT | grep -v "Duplicate report from database" > nreport
checkrv $?

check_nreport output.01

tdone
