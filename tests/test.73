#!/usr/bin/env bash

source common

DESC="scan(files8) hdd"

(cd ./files8 && ./create_files)

$DUPD_CMD scan --path `pwd`/files8 -q --hdd
checkrv $?

DESC="report"
$DUPD_CMD report --cut `pwd`/files8/ | grep -v "Duplicate report from database" > nreport
checkrv $?
FAILUSEROK=root
check_nreport output.72

(cd ./files8 && ./clean_files)

tdone
